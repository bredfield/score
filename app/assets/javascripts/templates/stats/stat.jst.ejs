
<div class="header blue"> 
	<span class="title">
		<%= stat.escape('name') %> 
		<% if(category){ %>
			<span class="category">
				(<%= category.escape('name') %>)
			</span>
		<% } %>
	</span>
</div>

<div class="main">
	<div class="controls">
		Controls
	</div><!-- .controls -->
	<div class="values">
		Values
	</div><!-- .values -->
	<ul class="menu">

			<li class="detail">
				<a class="detail" href="#stat/<%= stat.id %>">
					<img src="assets/icons/data.png" />
					<br />Detail
				</a>
			</li>

			<li>
				<a class="star" href="#">
					<img data-role="<%= stat.id %>" src="<%= star %>" />
					<br />Starred
				</a>
			</li>

			<li class="">
				<a class="options" href="#">
					<img src="assets/icons/settings.png" />
					<br />Options
				</a>
			</li>

			<li class="delete">
				<a class="delete" href="#">
					<img src="assets/icons/delete.png" />
					<br />Delete
				</a>
			</li>



	</ul>
</div><!-- .span3 -->
